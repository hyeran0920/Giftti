select * from gifticon_tbl;

select distinct brand from gifticon_tbl;


drop table if exists sale_tbl;

CREATE TABLE sale_tbl (
    register_Id INT AUTO_INCREMENT PRIMARY KEY, -- 등록번호 ID (기본키) 자동증가
    sale_price int NOT NULL,
    avail_date date NOT NULL,
    isSale ENUM('Available', 'Sold') NOT NULL DEFAULT 'Available',  -- 변경 사항 기본값설정
    inDate DATE DEFAULT (CURRENT_DATE),  -- 현재 등록 시간 바로 뜨게
    item_id INT NOT NULL,
    user_id varchar(20), -- 변경 사항
    FOREIGN KEY (user_id) REFERENCES user_tbl(user_id ) on delete set null,
    FOREIGN KEY (item_id) REFERENCES gifticon_tbl(item_id) on delete cascade on update cascade-- 기프티콘테이블이랑 참조키
       -- 유저테이블이랑 참조키
);

select * from sale_tbl;
select * from sale_tbl where isSale = 'Sold';

