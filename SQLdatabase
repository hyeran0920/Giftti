drop table if exists trans_tbl;

CREATE TABLE IF NOT EXISTS trans_tbl (
    trans_num INT AUTO_INCREMENT PRIMARY KEY, -- 거래번호 (기본키) 자동생성
    trans_date DATE NOT NULL, -- 거래일자
    register_id INT,  -- 등록번호
    buy_id varchar(20) NOT NULL, -- 구매유저ID 변경사항 
    FOREIGN KEY (register_id) REFERENCES sale_tbl(register_id) on delete set null, -- 판매테이블 참조키 -- 판매테이블 참조키
    FOREIGN KEY (buy_id) REFERENCES user_tbl(user_id) on update cascade-- 유저테이블 참조키
);

